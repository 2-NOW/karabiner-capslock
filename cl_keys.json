{
  "title": "CapsLock Enhancement - Keys",
  "rules": [
    {
      "description": "default keys",
      "manipulators": [
        {
          "description": "caps_lock + backspace to left_option + delete",
          "conditions": [
            {
              "name": "caps_lock_mode",
              "type": "variable_if",
              "value": 1
            },
            {
              "name": "caps_lock_space_mode",
              "type": "variable_if",
              "value": 0
            }
          ],
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {}
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": ["left_option"]
            }
          ],
          "type": "basic"
        },
        {
          "description": "caps_lock + a to select all",
          "conditions": [
            {
              "name": "caps_lock_mode",
              "type": "variable_if",
              "value": 1
            },
            {
              "name": "caps_lock_space_mode",
              "type": "variable_if",
              "value": 0
            }
          ],
          "from": {
            "key_code": "a",
            "modifiers": {}
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": ["left_gui"]
            }
          ],
          "type": "basic"
        },
        {
