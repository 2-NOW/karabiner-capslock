{
  "title": "CapsLock Enhancement - Shortcut",
  "rules": [
    {
      "description": "shortcut",
      "manipulators": [
        {
          "description": "caps_lock + 1 = vscode",
          "conditions": [
            { "name": "caps_lock_mode", "type": "variable_if", "value": 1 }
          ],
          "from": {
            "key_code": "1",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            { "shell_command": "open -a 'Visual Studio Code - Insiders.app'" },
            { "select_input_source": { "language": "en" } }
          ],
          "type": "basic"
        },

        {
          "description": "caps_lock + 2 = arc",
          "conditions": [
            { "name": "caps_lock_mode", "type": "variable_if", "value": 1 }
          ],
          "from": {
            "key_code": "2",
            "modifiers": { "optional": ["any"] }
          },
          "to": [{ "shell_command": "open -a 'Arc.app'" }],
          "type": "basic"
        },

